{
	"name": "server",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "ts-node-dev --respawn --transpile-only src/index.ts",
		"start:debug": "ts-node-dev --inspect=4321 --respawn --transpile-only index.ts",
		"start:dev": "cross-env NODE_ENV=development npm run start",
		"typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
		"typeorm:migration-create": "node_modules/.bin/ts-node node_modules/typeorm/cli.js migration:create -n",
		"typeorm:dropDB": "npm run typeorm schema:drop",
		"lint": "eslint . --ext .ts",
		"prepare": "cd .. && husky install server/.husky",
		"lint-fix": "eslint src/ --ext .tsx,.ts --fix && prettier src/ --write",
		"build": "tsc",
		"start:prod": "cd dist && node index.js"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"amqplib": "^0.8.0",
		"aws-sdk": "^2.959.0",
		"axios": "^0.21.1",
		"bcrypt": "^5.0.1",
		"cookie-parser": "^1.4.5",
		"cookie-session": "^1.4.0",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"express-validator": "^6.12.1",
		"joi": "^17.4.2",
		"jsonwebtoken": "^8.5.1",
		"lodash.get": "^4.4.2",
		"ms": "^2.1.3",
		"multer": "^1.4.2",
		"nodemailer": "^6.6.3",
		"passport": "^0.4.1",
		"passport-custom": "^1.1.1",
		"passport-github2": "^0.1.12",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"pg": "^8.6.0",
		"react-select": "^4.3.1",
		"reflect-metadata": "^0.1.13",
		"swagger-jsdoc": "^6.1.0",
		"swagger-ui-express": "^4.1.6",
		"typeorm": "^0.2.34",
		"typeorm-naming-strategies": "^2.0.0"
	},
	"devDependencies": {
		"@types/amqplib": "^0.8.1",
